{
  "apps": [
    {
      "name": "mus_api_staging",
      "script": "dist/main",
      "args": "start -p 3100",
      "time": true,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "instances": 2,
      "exec_mode": "cluster",
      "env": {
        "PORT": 3100,
        "NEXT_PUBLIC_API_URL": "https://api.staging-store.muselemu.com/api/v1",
        "API_URL": "https://api.staging-store.muselemu.com/api/v1",
        "FRONTEND_URL": "https://staging-store.muselemu.com",
        "DASHBOARD_URL": "https://dashboard.staging-store.muselemu.com"
      }
    }
  ]
}
