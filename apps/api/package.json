{
  "name": "@mus/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch --watchAssets",
    "dev:debug": "nest start --debug --watch --watchAssets",
    "start": "nest start",
    "start:prod": "node dist/main",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "db:reset": "ts-node -r tsconfig-paths/register src/database/db-reset.ts",
    "migration:run": "ts-node -r tsconfig-paths/register src/database/run-migrations.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show -d src/database/data-source.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "audit-ci": "audit-ci --config audit-ci.json",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "typecheck": "tsc --noEmit",
    "minio:start": "docker-compose -f docker-compose.minio.yml up -d",
    "minio:stop": "docker-compose -f docker-compose.minio.yml down",
    "minio:logs": "docker-compose -f docker-compose.minio.yml logs -f minio",
    "minio:reset": "docker-compose -f docker-compose.minio.yml down -v && docker-compose -f docker-compose.minio.yml up -d"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@mus/types": "*",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.11",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.11",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dd-trace": "^5.76.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.4.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "typeorm": "^0.3.21",
    "ua-parser-js": "^2.0.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.11",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.9.1",
    "@types/nodemailer": "^7.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
